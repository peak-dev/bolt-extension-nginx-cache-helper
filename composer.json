{
    "name": "bolt/nginx-cache-helper",
    "description": "Helper for flushing Nginx caches.",
    "type": "bolt-extension",
    "keywords": [
        "cache",
        "Nginx"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\NginxCacheHelper\\": "src"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bolt\\NginxCacheHelper\\NginxCacheHelperExtension",
        "bolt-icon": "icon.png",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}

